<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ly="https://www.coderbusy.com/luyao">
    <Design.PreviewWith>
        <StackPanel Spacing="10">
            <ly:FlagIcon Width="64" Code="CN" />
            <ly:FlagIcon Code="TW" />
            <ly:FlagIcon Code="HK" />
            <ly:FlagIcon Code="MO" />
            <ly:FlagIcon Code="US" />
            <ly:FlagIcon Code="GB" />
        </StackPanel>
    </Design.PreviewWith>
    <ControlTheme x:Key="{x:Type ly:FlagIcon}" TargetType="ly:FlagIcon">
        <Setter Property="Width" Value="22" />
    </ControlTheme>
</ResourceDictionary>
